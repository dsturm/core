{
    "name": "waterhole/core",
    "description": "Waterhole core",
    "keywords": [
        "forum"
    ],
    "license": "proprietary",
    "require": {
        "ext-json": "*",
        "actb/blade-github-octicons": "^4.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "dg/rss-php": "dev-master",
        "doctrine/dbal": "^3.1",
        "fntneves/laravel-transactional-events": "^2.0",
        "intervention/image": "^2.5",
        "jrmajor/fluent": "^0.3",
        "kirschbaum-development/eloquent-power-joins": "^2.4",
        "laravel/framework": "^8.45",
        "laravel/scout": "^9.1",
        "laravel/ui": "^3.3",
        "matthiasmullie/minify": "^1.3",
        "nyholm/psr7": "^1.4",
        "owenvoke/blade-fontawesome": "^1.9",
        "pragmarx/yaml": "^1.2",
        "s9e/text-formatter": "^2.11",
        "staudenmeir/eloquent-eager-limit": "^1.0",
        "staudenmeir/laravel-adjacency-list": "^1.7",
        "staudenmeir/laravel-cte": "^1.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "tonysm/turbo-laravel": "dev-invalid-forms-with-422",
        "vanderlee/php-stable-sort-functions": "^2.0",
        "wikimedia/less.php": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Waterhole\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "laravel": {
            "providers": [
                "Waterhole\\Providers\\WaterholeServiceProvider"
            ],
            "aliases": {
                "Waterhole": "Waterhole\\Waterhole"
            }
        }
    }
}
